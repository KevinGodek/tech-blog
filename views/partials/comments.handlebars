<div class="comments">
  {{#each this}}
  <div class="card">
    <h3 class="card-header"></h3>
    <section class="comment card-body">
      <div class="text">
        {{comment_id}}
      </div>
      <div class="meta">
        -{{user.username}}, {{formatDate created_at}}
      </div>
    </section>
  </div>
  {{/each}}
</div>